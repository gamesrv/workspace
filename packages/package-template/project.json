{
  "root": "packages/package-template",
  "sourceRoot": "packages/package-template/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/package-template",
        "main": "packages/package-template/src/index.ts",
        "tsConfig": "packages/package-template/tsconfig.lib.json",
        "assets": ["packages/package-template/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/package-template/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/package-template"],
      "options": {
        "jestConfig": "packages/package-template/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
